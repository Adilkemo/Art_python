from turtle import*

setup(700,700)

my_turtle=Turtle()
my_turtle.speed(30)
list=['violet','purple','blue','green','yellow','orange','red','blush','aquamarine','taupe']

def art(d, angle, x, y):
   for color in list:
      for i in range(1, 40):
         my_turtle.pencolor(color)
         my_turtle.circle(d)
         my_turtle.left(angle)
         d=d+1

art(50, 3, 0, 0)
